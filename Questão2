def fibonacci(n):
    a, b = 0, 1
    while b < n:
        a, b = b, a + b
    return a, b

num = int(input("Digite um número: "))

a, b = fibonacci(num)
if b ==  num:                                                                   
    print(f"{num} pertence à sequência de Fibonacci")
else:
    print(f"{num} não pertence à sequência de Fibonacci")
